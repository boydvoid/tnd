{
  "name": "tnd",
  "version": "0.0.1",
  "description": "The Teacher Next Door",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robaboyd/tnd.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/robaboyd/tnd/issues"
  },
  "homepage": "https://github.com/robaboyd/tnd#readme",
  "dependencies": {
    "concurrently": "^4.1.0",
    "draft-js": "^0.10.5",
    "express": "^4.16.4",
    "nodemon": "^1.19.0",
    "pure-react-carousel": "^1.19.4"
  }
}
